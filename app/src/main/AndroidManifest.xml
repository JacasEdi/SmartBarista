<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jc.sb.sb_customer_app">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.jc.sb.sb_customer_app.SplashScreenActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.jc.sb.sb_customer_app.LogoScreenActivity"
            android:screenOrientation="portrait"
            android:theme="@style/SplashScreenTheme" />
        <activity
            android:name="com.jc.sb.sb_customer_app.LoginScreenActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.jc.sb.sb_customer_app.CategoryMenuScreenActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.jc.sb.sb_customer_app.ProductMenuScreenActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.jc.sb.sb_customer_app.SingleProductScreenActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.jc.sb.sb_customer_app.ScheduleOrderScreenActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.jc.sb.sb_customer_app.VideoViewScreenActivity" />
        <activity android:name="com.jc.sb.sb_customer_app.RegisterScreenActivity" />
        <activity android:name="com.jc.sb.sb_customer_app.TopUpScreenActivity" />
        <activity android:name="com.jc.sb.sb_customer_app.ShareScreenActivity" />
        <activity android:name="com.jc.sb.sb_customer_app.ChangeUserDetailsScreenActivity" />
        <activity android:name="com.jc.sb.sb_customer_app.OrderHistoryScreenActivity"></activity>
    </application>

</manifest>